{
  "name": "zellij",
  "category": "general",
  "install_method": "github_release_binary",
  "description": "A terminal workspace with batteries included",
  "homepage": "https://github.com/zellij-org/zellij",
  "github_repo": "zellij-org/zellij",
  "binary_name": "zellij",
  "download_url_template": "https://github.com/zellij-org/zellij/releases/download/{version}/zellij-{arch}-unknown-linux-musl.tar.gz",
  "arch_map": {
    "x86_64": "x86_64",
    "aarch64": "aarch64"
  },
  "available_methods": [
    {
      "method": "github_release_binary",
      "priority": 1,
      "config": {
        "repo": "zellij-org/zellij",
        "asset_pattern": "zellij-x86_64-unknown-linux-musl.tar.gz"
      }
    },
    {
      "method": "cargo",
      "priority": 2,
      "config": {
        "crate": "zellij"
      }
    }
  ]
}
